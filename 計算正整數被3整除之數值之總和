#include <stdio.h>
#include <stdlib.h>

int main()
{
    int number;
    while(scanf("%d",&number)!=EOF)
    {
        int sum = 0;
        for(int i=1;i<=number;i++)
        {
            if(i%3==0)
            {
                sum = sum + i;
            }
        }
        printf("%d\n",sum);
    }
    return 0;
}
