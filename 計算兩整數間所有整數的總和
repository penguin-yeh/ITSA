#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num1,num2;
    while(scanf("%d%d",&num1,&num2)!=EOF)
    {
        if(num1>num2)
        {
            printf("%d\n",(num1-num2+1)*(num1+num2)/2);
        }
        if(num1<num2)
        {
            printf("%d\n",(num2-num1+1)*(num1+num2)/2);
        }
        else
        {
            printf("%d\n",num2);
        }
    }
    return 0;
}
